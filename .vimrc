syntax on
colorscheme default
" colorscheme torte

set number
set visualbell
set nocompatible

set wildmenu
set wildmode=longest:full,full

set hlsearch incsearch ignorecase

set wrap linebreak

set tabstop=4 shiftwidth=4 softtabstop=4
set noexpandtab

nnoremap j gj
nnoremap k gk

let g:netrw_liststyle=3
let g:netrw_banner=0

